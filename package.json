{
  "name": "hpa-model-zoo",
  "version": "0.1.2",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "check-format": "prettier --check \"{scripts,src,tests}/**/!(*@(.png))\"",
    "format": "prettier --write \"{scripts,src,tests}/**/!(*@(.png))\"",
    "lint": "vue-cli-service lint",
    "deploy": "npm run build && node scripts/deploy.js"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "file-saver": "^2.0.2",
    "marked": "^0.7.0",
    "vue": "^2.6.10",
    "vue-material": "^1.0.0-beta-11",
    "vue-router": "^3.1.2",
    "vuelidate": "^0.7.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.10.0",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "babel-eslint": "^10.0.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "gh-pages": "^2.1.1",
    "prettier": "1.17.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "@vue/prettier"
    ],
    "rules": {
      "no-console": "off",
      "vue/name-property-casing": [
        "error",
        "kebab-case"
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "prettier": {
    "trailingComma": "es5"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
